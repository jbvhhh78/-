-- 或者添加错误处理：
local success, err = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/jbvhhh78/-/main/ZiScript.lua", true))()
end)

if not success then
    warn("脚本加载失败: " .. err)
end

local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

WindUI.TransparencyValue = 0.3
WindUI:SetTheme("Dark")

-- 初始欢迎弹窗
WindUI:Popup({
    Title = "欢迎使用子脚本",
    Icon = "sparkles",
    Content = "尊贵的子脚本用户 "..game.Players.LocalPlayer.Name..", 测试",
    Buttons = {
        {
            Title = "启动",
            Icon = "arrow-right",
            Variant = "Primary",
            Callback = function() 
                print("脚本已启动")
            end
        }
    }
})

wait(10)

