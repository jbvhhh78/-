local function loadExternalScript(url, key, value, scriptName)
    WindUI:Notify({
        Title = "æç¤º",
        Content = "æ­£åœ¨åŠ è½½ " .. scriptName .. "...",
        Duration = 3
    })
    
    task.spawn(function()
        local success, err = pcall(function()
            -- è®¾ç½®ç¯å¢ƒå˜é‡
            if key and value then
                getgenv()[key] = value
            end
            
            -- åŠ è½½è„šæœ¬
            local scriptContent = game:HttpGet(url)
            loadstring(scriptContent)()
        end)
        
        if success then
            WindUI:Notify({
                Title = "æˆåŠŸ",
                Content = scriptName .. " åŠ è½½å®Œæˆ",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "å¤±è´¥",
                Content = scriptName .. " é”™è¯¯: " .. tostring(err),
                Duration = 5
            })
        end
    end)
end

