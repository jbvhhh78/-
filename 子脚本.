local function loadZiScript()
    local url = "https://raw.githubusercontent.com/jbvhhh78/-/af9f11f4693b4001a44968065c7ba0bb2e45bf3b/%E5%AD%90%E8%84%9A%E6%9C%AC.lua"
    
    print("å¼€å§‹åŠ è½½å­è„šæœ¬...")
    print("URL:", url)
    
    -- ç¬¬ä¸€æ­¥ï¼šè·å–å†…å®¹
    local success, content = pcall(function()
        return game:HttpGet(url, true)
    end)
    
    if not success then
        warn("âŒ è·å–è„šæœ¬å†…å®¹å¤±è´¥:", content)
        return false
    end
    
    print("âœ… è·å–å†…å®¹æˆåŠŸï¼Œé•¿åº¦:", #content)
    
    if #content < 10 then
        warn("âŒ è„šæœ¬å†…å®¹è¿‡çŸ­ï¼Œå¯èƒ½æ–‡ä»¶ä¸å­˜åœ¨æˆ–ä¸ºç©º")
        return false
    end
    
    -- ç¬¬äºŒæ­¥ï¼šæ‰§è¡Œè„šæœ¬
    local success, err = pcall(function()
        loadstring(content)()
    end)
    
    if not success then
        warn("âŒ æ‰§è¡Œè„šæœ¬å¤±è´¥:", err)
        return false
    else
        print("ğŸ‰ å­è„šæœ¬æ‰§è¡ŒæˆåŠŸï¼")
        return true
    end
end

loadZiScript()

